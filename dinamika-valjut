	<script type="text/javascript">
		function highlight(element) {
			var stars = $('.benchmark_rating span');
			var id = 0;
			for (var i = 0; stars[i] != element; i++) {
				$(stars[i]).addClass('active');
				id = i;
			}
			for (var i = id + 1; i <= stars.length; i++) {
				$(stars[i]).removeClass('active');
			}
			$(element).addClass('active');
		};

		function rate(rating) {
			var popup = window.open('/reyting?route=Markets/ExchangeRate/ExchangeRatePerformance&query=&rating=' + rating, '_blank', "width=500,height=300,scrollbars=no");
			popup.focus();
		};
	</script>

